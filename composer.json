{
  "name": "phpmodbus",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Chavez",
      "email": "bitclaw@gmail.com",
      "homepage": "http://www.bitclaw.com"
    }
  ],
  "support": {
    "issues": "https://github.com/bitclaw/phpmodbus/issues"
  },
  "description": "PHP Modbus Implementation",
  "keywords": ["phpmodbus","modbus"],
  "homepage": "https://github.com/bitclaw/phpmodbus",
  "require": {
    "php": "^5.3.2 || ^7.0",
    "ext-sockets": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0.5",
    "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0",
    "mockery/mockery": "^1.0@dev",
    "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
  },
  "autoload": {
    "psr-4": {
      "PHPModbus\\": "src/PHPModbus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPModbus\\Test\\": "tests/PHPModbus/Test"
    }
  },
  "config": {
    "discard-changes": true,
    "bin-dir": "bin"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev"
}
